<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Automation test suite" parallel="methods" thread-count="5" verbose="1">
  <test name="UI Automation Test">
    <parameter name="browser" value="${browser}"/>
    <parameter name="isLambdaTest" value="${isLambdaTest}"/>
    <parameter name="isHeadless" value="${isHeadless}"/>
    <classes>
       <class name="com.ui.test.LoginTest"/>
       <class name="com.ui.test.InvalidLoginCredentials"/>
       <class name="com.ui.test.AddNewFirstAddressTest"/>
       <class name="com.ui.test.ProductCheckoutTest"/>
       <class name="com.ui.test.SearchProductTest"/>
       
    </classes>
  </test>
</suite>